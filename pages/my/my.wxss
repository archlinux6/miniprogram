.container{
  background-color: #eeeeee;
  height : 100vh;
  padding-top: 0rpx;

  
}
.partOne {
  display: flex;
  justify-content: space-between;
  background-color: #a7d3e8;
  width: 100%;
  padding-top: 130rpx;
  padding-bottom: 130rpx;



}
.userinfo {
  display: flex;
  align-items: center;
  height: 128rpx;
  margin-left: 50rpx;
  color: rgb(255, 255, 255);
}

.userinfo-avatar {
  overflow: hidden;
  width: 128rpx;
  height: 128rpx;
  margin: 30rpx;
  border-radius: 50%;
}

.userinfo-nickname {
  height: 128rpx;
}


.partTwo{
  height: 300rpx;
  width: 90%;
  margin-left: 50rpx;
  margin-right: 50rpx;
  margin-top: -50rpx;
  margin-bottom: 5rpx;
  background-color: #ffffff;
  border: 1rpx solid rgb(176, 176, 176);
  border-radius: 25rpx;
}
.partTwo .item{
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 150rpx;
}
.partTwo .item .itemname{
  display: flex;
  justify-content: space-between;
  align-items: center;
  width: 210rpx;
  margin-left: 20rpx;
}
.partTwo .item .itemname image{
  width: 60rpx;
  height: 60rpx;
}

.partTwo .right{
  margin-right: 20rpx;
}
.partTwo .right image{
  width: 50rpx;
  height: 50rpx;
}

.partTwo .scan{
  display: flex;
  align-items: center;
  height: 100rpx;
}
.partTwo .scan .itemname{
  display: flex;
  justify-content: space-between;
  align-items: center;
  width: 210rpx;
  margin-left: 20rpx;
}
.partTwo .scan image{
  width: 60rpx;
  height: 60rpx;
}

